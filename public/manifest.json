{
  "manifest_version": 2,
  "name": "Chrome mkobayashime",
  "version": "0.1",
  "description": "Made for personal use. Use at your own risk.",
  "icons": {
    "16": "icons/icon_16.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [
        "quickTazugane.js"
      ]
    },
    {
      "matches": [
        "https://tweetdeck.twitter.com/"
      ],
      "run_at": "document_end",
      "js": [
        "tweetdeck.js"
      ]
    },
    {
      "matches": [
        "https://anime.dmkt-sp.jp/*"
      ],
      "run_at": "document_end",
      "js": [
        "dAnime.js"
      ]
    },
    {
      "matches": [
        "https://manaba.tsukuba.ac.jp/*"
      ],
      "run_at": "document_end",
      "js": [
        "manaba.js"
      ]
    },
    {
      "matches": [
        "http://www.onsen.ag/*"
      ],
      "run_at": "document_end",
      "js": [
        "onsen.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "style/tazugane.css"
  ]
}