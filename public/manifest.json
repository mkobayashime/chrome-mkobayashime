{
  "manifest_version": 2,
  "name": "Tweetdeck Util",
  "version": "0.1",
  "description": "Made for personal use. Use at your own risk.",
  "icons": {
    "16": "icons/icon_16.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://tweetdeck.twitter.com/"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}